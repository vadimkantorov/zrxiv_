{
	"manifest_version": 2,
	"name": "zrXiv",
	"version": "0.1",
	"content_scripts": [
		{
			"matches": [
				"https://arxiv.org/*"
			],
			"js": ["zrxiv.js"],
			"all_frames": true
		}
	],
	"web_accessible_resources": ["zrxiv.html"],
	"content_security_policy": "script-src 'self' 'unsafe-eval' 'unsafe-inline'; object-src 'self'"
}
